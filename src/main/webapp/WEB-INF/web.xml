<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>moneyQryServlet</servlet-name>
    <servlet-class>com.servlet.MoneyQryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>moneyQryServlet</servlet-name>
    <url-pattern>/servlet/moneyQryServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>transMoneyServlet</servlet-name>
    <servlet-class>com.servlet.TransMoneyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>transMoneyServlet</servlet-name>
    <url-pattern>/servlet/transMoneyServlet</url-pattern>
  </servlet-mapping>
</web-app>
